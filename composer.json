{
    "name": "religisaci/codeception-api-validator",
    "description": "Validate API Requests and Responses against Swagger / OpenAPI definitions. This package is modified awuttig/codeception-api-validator package for PHP 7.4",
    "version": "1.0.0",
    "authors": [
        {
            "name": "André Wuttig",
            "email": "wuttig@mailbox.org"
        },
        {
            "name": "Tomáš Hojgr",
            "email": "tomas.hojgr@religis.cz"
        }
    ],
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform-check": true,
        "platform": {
            "php": "7.4"
        },
        "vendor-dir": "build/vendor",
        "bin-dir": "build/bin"
    },
    "require":
    {
        "codeception/codeception": "*",
        "codeception/module-phpbrowser": "*",
        "codeception/module-rest": "*",
        "eleven-labs/api-validator": "*",
        "symfony/serializer": "*"
    }
}